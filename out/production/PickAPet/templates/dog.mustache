<!-- Primary Page Layout
 –––––––––––––––––––––––––––––––––––––––––––––––––– -->
<body>

<div class="section hero">
    <div class="container">
        <div class="row">
            <div class="one-half column">
                <h4 class="hero-heading">
                    {{#dogChoice}}

                    <H1 class="redheader">Congrats, you are a match with a <br>{{#dogChoice}}{{breed}}{{/dogChoice}}!</H1>


                </h4>

            </div>
            <div class="one-half column dog">
                <img class="pictures" src="pictures/{{breed}}.jpg">

            </div>
                    {{/dogChoice}}
        </div>
    </div>
</div>

<div class="section values">
    <div class="container">
        <div class="row">
            <div class="u-full-width">
                <h5 class="value-heading"><b>{{#dogChoice}}{{breed}}'s{{/dogChoice}} available for adoption in your local area!</b></h5>
            </div>

        </div>
    </div>
</div>

<div class="container">
    <div>
        {{#pets}}
            <div class="row" class="petinfo">
                <div><b>Name: {{name}}</b></div>
                <div>Gender: {{gender}}</div>
            </div>
            <div class="row">
                <div class="three columns">
                    {{phone}}<br /> {{contact}}
                    <img class="pictures" src="{{photo}}" />
                </div>
                <div class="nine columns petdesc"><pre>{{description}}</pre></div>
                <hr />
            </div>
            <hr />
            <div class="row"></div>
        {{/pets}}
    </div>
</div>

